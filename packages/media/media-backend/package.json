{
  "name": "@drax/media-back",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.36.0",
  "description": "Media files",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "prepublish": "tsc && npm run copygql",
    "tscrun": "tsc",
    "clean": "rm -rf dist",
    "copygql": "copyfiles -u 1 ./**/*.graphql dist/",
    "tsc": "tsc -b tsconfig.json",
    "test": "node --import tsx --test test/**/*.ts",
    "testCache": "node --import tsx --test test/cache/*"
  },
  "author": "Cristian Incarnato & Drax Team",
  "license": "ISC",
  "dependencies": {
    "@drax/common-back": "^0.36.0",
    "@drax/identity-back": "^0.36.0"
  },
  "peerDependencies": {
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.1.1",
    "better-sqlite3": "^11.0.0",
    "mongoose": "^8.3.4",
    "mongoose-paginate-v2": "^1.8.0",
    "mongoose-unique-validator": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5"
  },
  "gitHead": "d8e9ad20ff3a5bd9e9d4383ef0926b79eac335d8"
}
