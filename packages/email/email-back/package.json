  "devDependencies": {
    "@types/node": "^20.12.10",
    "@types/nodemailer": "^6.4.17",
    "@types/preview-email": "^3.1.0",
    "copyfiles": "^2.4.1",
    "mongoose-paginate-v2": "^1.8.3",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.0",
    "preview-email": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.2",
    "vitest": "^2.1.8"
