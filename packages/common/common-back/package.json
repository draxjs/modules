{
  "name": "@drax/common-back",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.9",
  "description": "Common utils across modules",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "serve": "nodemon --exec node --loader ts-node/esm src/index.ts",
    "prepublish": "tsc && npm run copygql",
    "tscrun": "tsc",
    "clean": "rm -rf dist",
    "copygql": "copyfiles -u 1 ./**/*.graphql dist/",
    "tsc": "tsc -b tsconfig.json",
    "test": "test"
  },
  "author": "Cristian Incarnato & Drax Team",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.4",
    "mongoose": "^8.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "tsc-alias": "^1.8.10"
  },
  "gitHead": "0b6b01d1116714843b943a91cdf9d3fbb361623b"
}
