{
  "name": "@drax/crud-back",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.2",
  "description": "Crud utils across modules",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "prepublish": "tsc && npm run copygql",
    "tscrun": "tsc",
    "clean": "rm -rf dist",
    "copygql": "copyfiles -u 1 ./**/*.graphql dist/",
    "tsc": "tsc -b tsconfig.json",
    "test": "node --import tsx --test test/**/*",
    "testService": "node --import tsx --test test/services/*.ts",
    "testExports": "node --import tsx --test test/exports/*.ts",
    "testWorkers": "node --import tsx --test test/workers/*.ts"
  },
  "author": "Cristian Incarnato & Drax Team",
  "license": "ISC",
  "dependencies": {
    "@drax/common-back": "^2.0.0",
    "@drax/common-share": "^2.0.0",
    "@drax/identity-share": "^2.0.0",
    "@drax/media-back": "^2.0.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.4",
    "mongoose": "^8.23.0",
    "mongoose-lean-virtuals": "^1.1.0",
    "mongoose-paginate-v2": "^1.8.3"
  },
  "peerDependencies": {
    "dayjs": "^1.11.19",
    "mongoose-paginate-v2": "^1.8.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "copyfiles": "^2.4.1",
    "mongoose-paginate-v2": "^1.8.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.9.3"
  },
  "gitHead": "e0ef450276ded01fd9e957e30ff18b409cd11fa9"
}
