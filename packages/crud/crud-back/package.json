{
  "name": "@drax/crud-back",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.12.1",
  "description": "Crud utils across modules",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "prepublish": "tsc && npm run copygql",
    "tscrun": "tsc",
    "clean": "rm -rf dist",
    "copygql": "copyfiles -u 1 ./**/*.graphql dist/",
    "tsc": "tsc -b tsconfig.json",
    "test": "node --import tsx --test test/**/*",
    "testService": "node --import tsx --test test/services/*.ts",
    "testExports": "node --import tsx --test test/exports/*.ts",
    "testWorkers": "node --import tsx --test test/workers/*.ts"
  },
  "author": "Cristian Incarnato & Drax Team",
  "license": "ISC",
  "dependencies": {
    "@drax/common-back": "^0.12.1",
    "@drax/common-share": "^0.12.1",
    "@drax/identity-share": "^0.12.1",
    "@drax/media-back": "^0.12.1",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.4",
    "mongoose": "^8.6.3",
    "mongoose-lean-virtuals": "^1.1.0",
    "mongoose-paginate-v2": "^1.8.3"
  },
  "peerDependencies": {
    "dayjs": "^1.11.13",
    "mongoose-paginate-v2": "^1.8.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "copyfiles": "^2.4.1",
    "mongoose-paginate-v2": "^1.8.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.2"
  },
  "gitHead": "fc38ec5cfa5715dbd946e97df02258f912ae00d0"
}
